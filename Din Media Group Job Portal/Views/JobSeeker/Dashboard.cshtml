@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}
<div id="wrapper">

    <!-- Header
    ================================================== -->
    <header class="sticky-header full-width">
        @Html.Partial("~/Views/Header/HeaderSelectionUser.cshtml")

    </header>
    <div class="clearfix"></div>
    <script>
        $(".sticky-header #logo a img").attr("src", "/Content/userContent/images/logo.png");
    </script>
    <!-- Titlebar
    ================================================== -->
    <div id="titlebar" class="single">
        <div class="container">

            <div class="sixteen columns">
                <h1>Candidate Dashboard</h1>
                <nav id="breadcrumbs" xmlns:v="http://rdf.data-vocabulary.org/#">
                    <ul>
                        <!-- Breadcrumb NavXT 5.6.0 -->
                        <li class="home"><span property="itemListElement" typeof="ListItem"><a property="item" typeof="WebPage" title="Go to WorkScout." href="https://workscout.in" class="home"><span property="name">WorkScout</span></a><meta property="position" content="1"></span></li>
                        <li class="current_item"><span property="itemListElement" typeof="ListItem"><span property="name">Candidate Dashboard</span><meta property="position" content="2"></span></li>
                    </ul>
                </nav>
            </div>
        </div>
    </div>

    <div class="jPanelMenu-menu container full-width">
       
            <div class="four columns">
                <nav id="navigation" class="menu">
                    <ul id="responsive">
                        <li class="is-active"><a href="https://workscout.in/job-dashboard/">Employer Dashboard </a></li>
                        <li class="">
                            <a href="https://workscout.in/my-account/">Dashboard</a>
                        </li>
                        <li class="">
                            <a href="https://workscout.in/my-account/orders/">Orders</a>
                        </li>
                        <li class="">
                            <a href="https://workscout.in/my-account/downloads/">Downloads</a>
                        </li>
                        <li class="">
                            <a href="https://workscout.in/my-account/edit-address/">Addresses</a>
                        </li>
                        <li class="">
                            <a href="https://workscout.in/my-account/edit-account/">Account Details</a>
                        </li>
                        <li class="">
                            <a href="https://workscout.in/my-account/customer-logout/">Logout</a>
                        </li>
                    </ul>
                </nav>
            </div>

            <div class="twelve columns">
                <div id="resume-manager-candidate-dashboard">
                    <p class="margin-bottom-25">Your resume can be viewed, edited or removed below.</p>
                    <table class="resume-manager-resumes manage-table resumes responsive-table">
                        <thead>
                            <tr>
                                <th class="resume-title"><i class="fa fa-user"></i> Name</th>
                                <th class="candidate-title"><i class="fa fa-file-text"></i> Title</th>
                                <th class="candidate-location"><i class="fa fa-map-marker"></i> Location</th>
                                <th class="resume-category">Category</th>
                                <th class="date"><i class="fa fa-calendar"></i> Date Posted</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td class="resume-title">
                                    Hafiz Mohsin <small>(Pending Review)</small>

                                </td>

                                <td class="candidate-title">
                                    web Developer
                                </td>

                                <td class="candidate-location">
                                    <span class="candidate-location">Lahore, Punjab, Pakistan</span>
                                </td>

                                <td class="resume-category">
                                    Web, Software &amp; IT
                                </td>

                                <td class="date">
                                    February 15, 2017
                                </td>

                                <td class="action">

                                    <a href="/candidate-dashboard/?action=delete&amp;resume_id=6925&amp;_wpnonce=4e385bda7f" class="candidate-dashboard-action-delete"><i class="fa fa-remove"></i> Delete</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <br>

                    <a class="button" href="https://workscout.in/submit-resume/">Add Resume</a>

             
            </div>
</div>
    </div>

    <!-- Footer
    ================================================== -->
    <div class="margin-top-25"></div>

    @Html.Partial("~/Views/Footer/_partialUserFooter.cshtml")

    <!-- Back To Top Button -->
    <div id="backtotop"><a href="#"></a></div>

</div>
<!-- Wrapper / End -->
