@{
    try
    {
        Din_Media_Group_Job_Portal.Models.tb_user user;
        user = (Din_Media_Group_Job_Portal.Models.tb_user)Session["user"];
        if (user != null)
        {
            if (user.user_type == "employee")
            { 
                @Html.Partial("~/Views/Header/_partialLoggedInUserHeader.cshtml")
        
            }
            else if (user.user_type == "employer")
            {           
                @Html.Partial("~/Views/Header/_partialLoggedInEmployerHeader.cshtml")
            }
        }
        
        else
        {
            @Html.Partial("~/Views/Header/_partialUserHeader.cshtml")
        }    
    }
    catch(Exception e)
    {
        @Html.Partial("~/Views/Header/_partialUserHeader.cshtml")
    }
   
}